{
  "compilerOptions": {
    "target": "es2015", // Eller högre, t.ex. "es2017" eller "esnext"
    "lib": ["dom", "es2015"], // Lägg till relevanta bibliotek
    "downlevelIteration": true, // Tillåt iteration över iterable objekt
    "allowJs": true,
    "skipLibCheck": true,
    "checkJs": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/components/parksComponent/parkmap.jsx"],
  "exclude": ["node_modules"]
}
